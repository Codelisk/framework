<Project Sdk="MSBuild.Sdk.Extras">

    <PropertyGroup>
        <Description>A collection of libraries and classes to make Xamarin, Shiny, RXUI, and Prism play beautifully together</Description>
        <TargetFramework>netstandard2.0</TargetFramework>
        <RootNamespace>Shiny</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="DryIoc.Microsoft.DependencyInjection" Version="5.1.0" />
        <PackageReference Include="Prism.DryIoc.Forms" Version="8.1.97" />
        <PackageReference Include="ReactiveUI" Version="17.1.9" />
        <PackageReference Include="ReactiveUI.Fody" Version="17.1.9" />
        <PackageReference Include="Shiny.Core" Version="2.4.1.2971" />
        <PackageReference Include="Xamarin.Essentials" Version="1.7.0" />
        <PackageReference Include="Xamarin.Forms" Version="5.0.0.2244" />
        <PackageReference Include="XF.Material" Version="1.7.8" />
        <PackageReference Include="Rg.Plugins.Popup" Version="2.0.0.14" />
    </ItemGroup>

</Project>
