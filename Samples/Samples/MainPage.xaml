<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Samples.MainPage"
             Title="{Binding Title}">

    <StackLayout>
        <Button Text="HI" Command="{Binding Button}" />

        <Button Text="MSAL Sign-In" Command="{Binding MsalSignIn}" />
        <Button Text="MSAL Sign-Out" Command="{Binding MsalSignOut}" />
        <Button Text="MSAL Refresh" Command="{Binding MsalRefresh}" />
    </StackLayout>

</ContentPage>
